import java.util.Scanner;
public class Main
{
	public static void main(String[] args)
	{
	    Scanner obj= new Scanner(System.in); 
		String name;
	    int age;
		name="Pranaw";
	    System.out.print("Enter your age =");
	    age=obj.nextInt();
		System.out.println("Name ="+name+"\n"+"Age ="+age+"\n");
	}
}
