import java.util.Scanner;
public class Main
{
	public static void main(String[] args)
	{
	    Scanner obj= new Scanner(System.in); 
	    int num;
	
	    System.out.print("Enter number =");
	    num=obj.nextInt();
		System.out.println("num is"+num);
		if(num%2==0)
		{
		    System.out.println("even");
		}
		else
		{
		    System.out.println("odd");
		}
	}
}
